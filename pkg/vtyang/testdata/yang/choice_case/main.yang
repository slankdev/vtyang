module main {
  namespace "http://slank.dev/vtyang";
  prefix main;

  container values {
    container transport-proto {
      choice transport-proto-choice {
        case tcp-case {
          leaf tcp-app {
            type string;
          }
        }
        case udp-case {
          leaf udp-app {
            type string;
          }
        }
      }
    }
  }
}
